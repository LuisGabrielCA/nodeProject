		<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
        <link rel="stylesheet" href="../public/assets/css/styleMenu.css">
        <link rel="stylesheet" href="../public/assets/css/styleForm.css">
        
  </head>
  <body>
		
    <div class="wrapper d-flex align-items-stretch">
        <nav id="sidebar">
            <div class="custom-menu">
                <button type="button" id="sidebarCollapse" class="btn btn-primary">
          <i class="fa fa-bars"></i>
          <span class="sr-only">Toggle Menu</span>
        </button>
    </div>
            <div class="p-4 pt-5">
              <h5 id="linha">.</h5>
              <h5 id="linha">.</h5> 
        <ul class="list-unstyled components mb-5">
          <li class="active">
            <a href="/home" aria-expanded="false" class="">Home</a>
            <ul class="collapse list-unstyled" id="homeSubmenu">
            </ul>
          </li>
          <li>
              <a href="/addUser">Cadastrar</a>
          </li>
          <li>
            <a href="/search" aria-expanded="false" class="">Pesquisar</a>
          <ul class="collapse list-unstyled" id="pageSubmenu">
          </ul>
          </li>
          <li>
          <a href="/filterBr">Filtrar</a>
          </li>
  
        </ul>
    
    </nav>

        <!-- Page Content  -->
      <div id="content" class="p-4 p-md-5 pt-5">

        <h2 class="mb-4">Resultado da Pesquisa</h2>

        <table class="table" id="tabela">
        <thead class="thead-dark">
            <tr>
              <th scope="col">ID</th>
              <th scope="col">NOME</th>
              <th scope="col">BAIRRO</th>
              <th scope="col">REGI√ÉO</th>
              <th scope="col">TELEFONE</th>
              <th scope="col">CELULAR</th>
              <th scope="col">SEXO</th>
              <th scope="col">DATA DE NASCIMENTO</th>
              <th scope="col">DETALHES</th>
              </tr>
            </thead>
    <% cadastro.forEach(function(user){ %>
      
            <tbody>
              <tr>
                <td><%= user.idusuarios %></td>
                <td><%= user.nome %></td>
                <td><%= user.bairro %></td>
                <td><%= user.regiao %></td>
                <td><%= user.telefone %></td>
                <td><%= user.celular %></td>
                <td><%= user.sexo %></td>
                <td><%= user.dtnascimento %></td>
                <td><a href='listUser/<%= user.idusuarios%>' type="button" class="btn btn-primary">+ Detalhes</a></td>
              </tr>
            </tbody>

    <% }) %>
        </table>

        </div>

  <script src="../public/assets/js/jquery.min.js"></script>
  <script src="../public/assets/js/popper.js"></script>
  <script src="../public/assets/js/bootstrap.min.js"></script>
